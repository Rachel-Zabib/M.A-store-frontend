is vitae eos possimus delectus tempore. Quas veniam, distinctio facere modi culpa recusandae aliquid consequatur? Atque recusandae perferendis ipsum facere hic.\r\n                  </div>\r\n               </div>\r\n               <hr/>\r\n               <div><b>similar product</b><span id=\"span3\" onClick={this.toggleMoreDiv.bind(this)} className=\"moreSpan\">+</span><br/>\r\n                  <div className={this.state.span3}>\r\n                  {this.state.theProduct.similarProducts}\r\n                  </div>\r\n               </div>\r\n            </div>\r\n            <div className=\"imagesProduct\">\r\n               <div className=\"imageProductDisplay\">\r\n                  <img src={this.state.imgUrlDisplay||this.state.theProduct.imgSrc[0]}></img>\r\n               </div>\r\n               <ul className=\"galleryProduct\">\r\n                  {(this.state.theProduct.imgSrc).map((v,k)=>{\r\n                     console.log(v);\r\n                     return  <li key={k} onClick={(e)=>this.changeUrlDisplay(e.target.src)}><img src={v}></img></li>\r\n                  })}  \r\n               </ul>\r\n             \r\n               \r\n            </div>\r\n         </div>\r\n            \r\n      );\r\n   }\r\n}\r\nexport default ProductPage;\r\n\r\n{/* <i class=\"fas fa-heart\"></i>   love full icon*/}","C:\\Users\\rache\\Documents\\תרגילים EXPERIS\\final_project\\reactProject2\\reactproject2\\src\\components\\Header\\Header.js",["237","238","239","240","241","242","243","244"],"C:\\Users\\rache\\Documents\\תרגילים EXPERIS\\final_project\\reactProject2\\reactproject2\\src\\components\\StorePage\\StorePage.js",["245","246","247","248","249","250","251","252","253","254","255","256","257"],"import React, { Component } from 'react';\r\nimport GridProduct from '../GridProduct/GridProduct';\r\nimport ListCategory from '../ListCategory/ListCategory'\r\nimport HeaderCategory from '../HeaderCategory/HeaderCategory'\r\nimport './storePage.css';\r\nimport prop